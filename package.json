{
  "name": "deriv-icons",
  "version": "0.0.1",
  "description": "This is the central repository for deriv icons, exported from figma design file",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "jsdelivr": "sprite/icons.svg",
  "cdn": "sprite/icons.svg",
  "unpkg": "sprite/icons.svg",
  "scripts": {
    "clean:stories": "rimraf stories storybook-static dist",
    "clean:svg": "rimraf svg",
    "clean": "rimraf stories src svg storybook-static sprite",
    "export:figma": "ts-node ./node_modules/@figma-export/cli/bin/run use-config ./scripts/figma.config.ts",
    "export:barrels": "barrelsby -c barrelsby.config.json",
    "export": "npm run clean && npm run export:figma && npm run export:barrels && npm run format",
    "bundle": "rimraf dist && rollup -c",
    "build": "npm-run-all clean export bundle",
    "build_and_publish": "npm run build && npm publish",
    "storybook": "start-storybook -p 6006",
    "start": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "eslint --fix src/**/*.{js,jsx,ts,tsx,json}",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json,scss} --config ./.prettierrc",
    "prepare": "husky install"
  },
  "keywords": [],
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@figma-export/cli": "^4.5.0",
    "@figma-export/output-components-as-es6": "^4.5.0",
    "@figma-export/output-components-as-svg": "^4.5.0",
    "@figma-export/output-components-as-svgr": "^4.5.0",
    "@figma-export/output-components-as-svgstore": "^4.5.0",
    "@figma-export/transform-svg-with-svgo": "^4.5.0",
    "@figma-export/types": "^4.5.0",
    "@figma-export/utils": "^4.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.1.0",
    "@rollup/plugin-typescript": "^10.0.0",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-webpack4": "^6.5.13",
    "@storybook/manager-webpack4": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "babel-loader": "^8.3.0",
    "barrelsby": "^2.5.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.5.0",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "react": ">= 16",
    "react-dom": ">= 16"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.prettierrc": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/binary-com/deriv-icons.git"
  },
  "author": "Mohsen Hajibeigloo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/binary-com/deriv-icons/issues"
  },
  "homepage": "https://github.com/binary-com/deriv-icons#readme"
}
